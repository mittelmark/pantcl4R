digraph G {
graph[rankdir=LR];
subgraph cluster_0 {
  style=filled;
  color=lightgrey;
  label="process #1";
  node[style=filled, color=white];
  a0 -> a1 -> a2 -> a3;
}
subgraph cluster_1 {
  label="process #2";
  color=blue;
  node[style=filled];
  b0 -> b1 -> b2 -> b3;
}
  start -> a0;
  start -> b0;
  a1 -> b3 -> end;
  b2 -> a3 -> end;
  a3 -> a0;
  start[shape=Mdiamond];
  end[shape=Msquare];

}

