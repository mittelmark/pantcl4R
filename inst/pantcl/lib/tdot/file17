  node[shape=box, style=filled, fillcolor=grey, width=1];
  1988 -> 1993 -> 1995 -> 1997 -> 1999 -> 2000 -> 2002 -> 2007 -> 2012 -> future;
  node[fillcolor="#ff9999"];

