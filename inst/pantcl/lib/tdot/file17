  8.0 -> Namespace[dir=back];
  Unicode -> 8.1;
  8.1 -> Wiki;
  TEA -> 8.3;
  8.3 -> Tcllib -> Tklib;
  8.4 -> Starkit -> Snit -> Dict -> 8.5;
  vfs -> 8.4;
  Tile -> 8.5;
  TclOO -> 8.6 -> TDBC;
  zipvfs -> 8.7;
{
rank=same; 1988; "Tcl/Tk"[group=g1];
}
{
rank=same; 1993; 7.3[group=g1]; Itcl;
}
{
rank=same; 1995; Tk[group=g0]; 7.4[group=g1]; Otcl[group=g2];
