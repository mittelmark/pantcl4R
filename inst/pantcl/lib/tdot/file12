strict digraph G {
graph[margin=0.4];
  node[style=filled, fillcolor=grey80, width=1.2, height=0.7];
{
rank=same; E; D; C; F; G;
}
  A -> B[label=" connects"];
  B -> C;
  B -> D;
  D -> E;
  A[label="Hello", style=filled, fillcolor=salmon, width=2, height=1];
  B[label="World!", style=filled, shape=box, fillcolor=skyblue, width=2, height=0.8];
  C -> F -> G;

}

